{
    "name": "file-uploader-project",
    "version": "1.0.0",
    "description": "File Uploader with Rust, WASM, Node.js, and React",
    "scripts": {
        "start": "concurrently \"npm run start:server\" \"npm run start:client\"",
        "start:server": "node server/server.js",
        "start:client": "cd client && npm start",
        "build": "cd client && npm run build",
        "test": "cd client && npm test"
    },
    "dependencies": {
        "concurrently": "^6.2.0",
        "express": "^4.19.2",
        "multer": "^1.4.4-lts.1",
        "ws": "^7.5.10"
    },
    "devDependencies": {
        "nodemon": "^2.0.7"
    }
}
